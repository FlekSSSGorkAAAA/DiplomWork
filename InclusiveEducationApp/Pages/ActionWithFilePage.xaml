<Page x:Class="InclusiveEducationApp.Pages.ActionWithFilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:InclusiveEducationApp.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ActionWithFilePage">

    <Grid Background="White">
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="makeCountOperation" Content="Произвести расчёт" Width="250" Height="30" Click="makeCountOperation_Click"/>
            <Button x:Name="makeLoadInfo" Content="Совершить выгрузку" Width="250" Height="30" Click="makeLoadInfo_Click" Margin="0 20 0 0"/>
        </StackPanel>
    </Grid>
</Page>
